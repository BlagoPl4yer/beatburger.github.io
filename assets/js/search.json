---
layout: none
---
[
  {% for bot in site.bots %}
    {
      "title"    : "{{ bot.botName | escape }}",
      "category" : "{{ bot.category }}",
      "tags"     : "{{ bot.tags | join: ', ' }}",
      "url"      : "/{{ bot.botName | slugify }}",
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
